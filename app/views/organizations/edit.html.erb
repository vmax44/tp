<h1>Edit Organization</h1>

<div class="row">
  <div>
    <%= form_for(@organization) do |f| %>
      
      <%= render 'fields', f: f %>
      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
    
    <% @organization.people.each do |p| %>
    <p><%=p.firstname%> <%=p.lastname%> </p>
    <% end %>
  </div>
</div>